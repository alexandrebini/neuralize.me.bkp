		 xmlns:grid1="me.neuralize.datainsert.view.components.grid.*" 
		 xmlns:ns1="me.neuralize.datainsert.view.components.grid.controllers_grid.*" 
		 xmlns:containers="me.neuralize.app.view.components.util.containers.*"

		 title="Data insert"
		 width="100%" 
		 height="100%" 
		 paddingTop="0" paddingLeft="0" paddingRight="0" paddingBottom="0"
		 >
